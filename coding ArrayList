package myArray;

public class ArrayList {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] list = new int[10];
		
		list [3] = 10;
		list [6] = 35;
		list [5] = list [3] + list [6];
		
		System.out.println(list [3]);
		System.out.println(list [6]);
		System.out.println(list [5]);
		
	
	}
}
